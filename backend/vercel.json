{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "env": {
    "PORT": "5000",
    "CLOUDINARY_CLOUD_NAME": "",
    "CLOUDINARY_API_KEY": "",
    "CLOUDINARY_API_SECRET": "",
    "JWT_SECRET": "",
    "REACT_APP_ALLOWED_ORIGINS": "http://localhost:3000",
    "REACT_APP_BACKEND_URL": "https://skill-share-odoo.vercel.app"
  }
}
