{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://skill-share-odoo.vercel.app/api/$1"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "https://skill-share-odoo-lcrc.vercel.app/api"
  }
}
